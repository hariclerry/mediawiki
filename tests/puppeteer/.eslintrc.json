{
	"root": true,
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 8
	},
	"extends": [
		"../../.eslintrc.json",
		"plugin:jest/recommended"
	],
	"plugins": [ "jest" ],
	"env": {
		"node": true,
		"browser": true,
		"jest/globals": true
	},
	"rules": {
		"jest/no-jasmine-globals": 0
	}
}
